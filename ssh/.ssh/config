# https://linuxize.com/post/using-the-ssh-config-file/

Host *
    IgnoreUnknown UseKeychain
    UseKeychain yes
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa

Host gluu.open.org.kh
    HostName gluu.open.org.kh
    User admin
    IdentitiesOnly yes
    IdentityFile ~/.ssh/gluu.open.org.kh.pem
