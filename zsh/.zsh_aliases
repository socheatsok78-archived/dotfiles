# Aliases
alias .2='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'
alias .6='cd ../../../../../..'
alias reload='source ~/.zshrc'

# fs
alias ls='ls -GpF'
alias ll='ls -alGpF'

# Tools
# - ignore alias: \cat or command cat
alias cat='bat'
alias ping='prettyping --nolegend'
alias top='htop'
